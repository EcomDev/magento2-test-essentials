{
  "name": "ecomdev/magento2-test-essentials",
  "description": "Magento 2 Test Essentials",
  "type": "library",
  "require": {
    "magento/framework": "~103.0",
    "magento/module-catalog": "~104.0",
    "magento/module-quote": "~101.0",
    "magento/module-customer": "~103.0",
    "magento/module-store": "~101.0",
    "php": "~8.1.0||~8.2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^11.5",
    "brianium/paratest": "^7.7"
  },
  "license": [
    "MIT"
  ],
  "keywords": [
    "magento2", "testing", "fake objects"
  ],
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "autoload": {
    "psr-4": {
      "EcomDev\\Magento2TestEssentials\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/fixtures.php"
    ],
    "psr-4": {
      "EcomDev\\Magento2TestEssentials\\": "tests"
    }
  },
  "scripts": {
    "test": "XDEBUG_MODE=coverage paratest --coverage-text",
    "format:check": "phpcs",
    "format:write": "phpcbf"
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": false
    }
  }
}
